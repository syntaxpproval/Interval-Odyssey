// Game tileset (our game tiles)
const unsigned char game_tileset[] = {
    // Floor tile (empty with dots)
    0x00, 0x00, 0x00, 0x00, 0x04, 0x04, 0x00, 0x00,
    0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00, 0x00,
    
    // Wall tiles (16x16 split into four 8x8)
    // Top-left piece
    0xFF, 0xFF, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81,
    0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81,
    
    // Top-right piece
    0xFF, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF,
    0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF,
    
    // Bottom-left piece
    0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81,
    0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0xFF,
    
    // Bottom-right piece
    0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF,
    0x81, 0xFF, 0x81, 0xFF, 0x81, 0xFF, 0xFF, 0xFF
};

// Example implementation for loading tileset data
void load_tileset(void) {
    set_bkg_data(0, sizeof(tileset_data) / 16, tileset_data);
}

// Example implementation for drawing text using the tileset
void draw_text(UINT8 x, UINT8 y, const char *string) {
    UINT8 i = 0;
    while (string[i] != '\0') {
        set_bkg_tiles(x + i, y, 1, 1, &string[i]);
        i++;
    }
}